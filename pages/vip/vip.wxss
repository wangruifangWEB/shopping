@import "../tpls/search/search.wxss";
@import "../tpls/swiper/swiper.wxss";

.vip-card {
  width: 90%;
  margin: 0 auto;
  position: relative;
}

.vip-top {
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  padding: 40rpx 20rpx;
  background: #ff5b62 url(data:image/png;base64,) no-repeat;
  background-size: auto 100%;
  background-position-x: 70%;
}

.user-box {
  display: flex;
  align-items: center;
  width: 80%;
}

.vip-img {
  width: 90rpx;
  margin-right: 10rpx;
}

.vip-smile {
  width: 60rpx;
}

.name text {
  font-size: 28rpx;
  color: #fff;
}

.name text:last-of-type {
  font-size: 20rpx;
  padding: 8rpx 20rpx;
  background: #fff;
  color: #ff5b62;
  display: inline-block;
  border-radius: 20rpx;
  margin-left: 30rpx;
  font-style: oblique;
}

.id {
  display: block;
}

.id text {
  font-size: 28rpx;
  color: #fff;
}

.id-line {
  padding: 0 15rpx;
}

.vip-bottom {
  display: flex;
  align-items: center;
  padding: 40rpx 20rpx;
  background: #ffe7e9 url(data:image/png;base64,) no-repeat;
  background-size: auto 90%;
  background-position-x: 90%;
  padding-bottom: 30rpx;
}

.container {
  background-color: #fff;
}

.vip-bottom-text text {
  display: block;
  color: #333;
  font-size: 28rpx;
  text-align: left;
  margin-left: 15rpx;
  line-height: 1.8;
}

.coupon {
  position: relative;
  z-index: 999;
  display: flex;
  align-items: center;
  justify-content: space-around;
  text-align: center;
  padding: 30rpx 40rpx;
  border-top-left-radius: 25rpx;
  border-top-right-radius: 25rpx;
  box-shadow: 0 -4rpx 15rpx rgba(0, 0, 0, 0.1);
  background: #fff;
}

.coupon image {
  width: 50rpx;
  text-align: center;
}

.coupon text {
  color: #999;
  font-size: 24rpx;
  display: block;
  margin-top: 5rpx;
}

.coupon-line {
  width: 3rpx;
  height: 60%;
  color: #c2c2c2;
  display: block;
}

.vip-box {
  background: #f3f3f3;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.h1 {
  font-size: 48rpx;
  color: #2f2f2f;
  text-align: center;
  line-height: 2.5;
  display: block;
  background: #fff;
}

.swiper {
  height: 130px;
}

.goods-box {
  width: 90%;
  margin: 0 auto;
  margin-top: 30rpx;
}

.goods-box::after {
  content: '';
  display: block;
  clear: both;
}

.goods-list {
  width: 48%;
  float: left;
  margin-bottom: 4%;
}

.goods-list:nth-of-type(2n) {
  float: right;
}

.goods-list image {
  display: block;
  width: 100%;
  height: 230rpx;
  border-top-right-radius: 15rpx;
  border-top-left-radius: 15rpx;
}

.goods-list text {
  display: block;
  text-align: center;
  padding: 10rpx;
  background: #fff;
  line-height: 2;
  font-size: 28rpx;
  color: #ff5b62;
  border-radius: 10rpx;
}

.noLogin {
  font-size: 26rpx;
  background-color: #fff;
  padding: 0 10rpx;
  border-radius: 10rpx;
  margin-top:10rpx;
  opacity: 0.5;
}

.fontColor {
  color: #fff;
}

.nickName {
  color:#fff;
  width: 40%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display:inline-block;
  font-size: 26rpx;
}

.btn-hover{
  background-color: #fff
}

.fontShow{
  text-align: center;
  margin:30rpx 0;
}